
! Fonts {{{
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.hintstyle: hintfull
Xft.dpi:       280
! }}}


Xcursor.theme: DMZ-White

URxvt.font:                xft:Source Code Pro:pixelsize=32:antialias=true
URxvt.boldFont:            xft:Source Code Pro:pixelsize=32:bold:antialias=true
URxvt.italicFont:          xft:Source Code Pro:pixelsize=32:italic:antialias=true
URxvt.boldItalicFont:      xft:Source Code Pro:pixelsize=32:bold:italic:antialias=true
!URxvt.letterSpace: 	   -6

URxvt.transparent: true
URxvt.shading: 17

! X11 colors pieced together by Dmitry Medvinsky.
! Based on Vim colorscheme "badwolf" pieced together by Steve Losh.

*background: #1c1b1a
*foreground: #f8f6f2
*cursorColor: #aeee00
*scrollColor: #45413b
*fadeColor: #ffffff
*fading: 20

! black
*color0: #141413
*color8: #000000
! red
*color1: #ff2c4b
*color9: #ff2c4b
! green
*color2: #aeee00
*color10: #aeee00
! yellow
*color3: #fade3e
*color11: #ffa724
! blue
*color4: #0a9dff
*color12: #0a9dff
! magenta
*color5: #ff9eb8
*color13: #ff9eb8
! cyan
*color6: #8cffba
*color14: #8cffba
! white
*color7: #d9cec3
*color15: #ffffff

 URxvt.scrollBar:           false


! rofi settings
rofi.fake-transparency:              true
